{
    "grouped": true,
    "title": "設定",
    "controllerName": "luxeysSettingViewController",
    "sections": [
        { "title":"プロフィール設定", "elements":[
                { "type":"QEntryElement", "key":"name", "title":"ニックネーム", "Placeholder":"ニックネーム", "bind":"textValue:name"},
                { "type":"QRadioElement", "key":"gender", "title":"性別", "items":["男性", "女性"], "values":[1, 2], "controllerAction":"handleUpdate:"},
                { "type":"QDateTimeInlineElement", "key":"birthday", "title":"生年月日"},
                { "type":"QEntryElement", "key":"current_residence", "title":"現住所", "Placeholder":"現住所", "bind":"textValue:current_residence"},
                { "type":"QEntryElement", "key":"hometown", "title":"出身地", "Placeholder":"出身地", "bind":"textValue:hometown"},
                { "type":"QEntryElement", "key":"occupation", "title":"職業", "Placeholder":"職業", "bind":"textValue:occupation"},
                { "type":"QMultilineElement", "key":"introduction", "title":"自己紹介", "bind":"textValue:introduction"},
                { "type":"QMultilineElement", "key":"hobby", "title":"趣味", "bind":"textValue:hobby"}
            ]
        },
        { "title":"公開設定", "elements": [
                { "type":"QRadioElement", "key":"picture_status", "title":"写真", "items":["非公開", "友達まで", "会員まで", "全体"], "values":[0, 10, 30, 40], "controllerAction":"handleUpdate:"},
                { "type":"QRadioElement", "key":"gender_public", "title":"性別公開", "items":["非公開", "友達まで", "会員まで", "全体"], "values":[0, 10, 30, 40], "controllerAction":"handleUpdate:"},
                { "type":"QRadioElement", "key":"current_residence_public", "title":"現住所公開", "items":["非公開", "友達まで", "会員まで", "全体"], "values":[0, 10, 30, 40], "controllerAction":"handleUpdate:"},
                { "type":"QRadioElement", "key":"hometown_public", "title":"出身地公開", "items":["非公開", "友達まで", "会員まで", "全体"], "values":[0, 10, 30, 40], "controllerAction":"handleUpdate:"},
                { "type":"QRadioElement", "key":"birthyear_public", "title":"年齢公開", "items":["非公開", "友達まで", "会員まで", "全体"], "values":[0, 10, 30, 40], "controllerAction":"handleUpdate:"},
                { "type":"QRadioElement", "key":"birthdate_public", "title":"誕生日公開", "items":["非公開", "友達まで", "会員まで", "全体"], "values":[0, 10, 30, 40], "controllerAction":"handleUpdate:"}
            ]
        },
        { "elements":[
                { "type":"QButtonElement", "title":"ログアウト", "controllerAction":"handleLogout:"},
            ]
        },
        { "elements":[
                { "type":"QButtonElement", "title":"機能要望・ご意見", "controllerAction":"handleFeedback:"},
            ]
        }
    ]
}