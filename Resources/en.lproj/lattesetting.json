                { "type":"QRadioElement", "key":"birthdate_public", "title":"Birthday", "items":["Me only", "Friends only", "Members only", "Public"], "values":[0, 10, 30, 40], "controllerAction":"handleUpdate:"}
            ]
        },
        { "elements":[
                { "type":"QButtonElement", "title":"LOGOUT", "controllerAction":"handleLogout:"},
            ]
