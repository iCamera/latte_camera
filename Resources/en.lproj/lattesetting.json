                { "type":"QEntryElement", "key":"hometown", "title":"Hometown", "Placeholder":"Hometown", "bind":"textValue:hometown"},
                { "type":"QEntryElement", "key":"occupation", "title":"Occupation", "Placeholder":"Occupation", "bind":"textValue:occupation"},
                { "type":"QMultilineElement", "key":"introduction", "title":"Introduction", "bind":"textValue:introduction"},
                { "type":"QMultilineElement", "key":"hobby", "title":"Hobby", "bind":"textValue:hobby"}
            ]
